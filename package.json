{
  "name": "lyfta",
  "version": "1.0.0",
  "description": "A weightlifting meet management system",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "build": "tsc -p . && ncp src/schema dist/schema"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J-Wood105/Lyfta.git"
  },
  "keywords": [
    "weightlifting"
  ],
  "author": "JB Wood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/J-Wood105/Lyfta/issues"
  },
  "homepage": "https://github.com/J-Wood105/Lyfta#readme",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "bluebird": "^3.5.5",
    "compression": "^1.7.4",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "ejs": "^2.6.2",
    "express": "^4.17.3",
    "express-session": "^1.16.2",
    "graphql": "^14.4.2",
    "http": "0.0.0",
    "mongoose": "^5.7.5",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/compression": "^1.0.0",
    "@types/connect-mongo": "0.0.41",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/graphql": "^14.2.3",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/node": "^12.7.1",
    "dotenv": "^7.0.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "0.0.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
